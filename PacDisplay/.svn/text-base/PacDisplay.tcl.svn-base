#
#  Append PacDisplay to the sequence and configure it
#
sequence append MyAnalysisSequence PacDisplay
mod enable PacDisplay
talkto PacDisplay {
	fileName set $HistFileName
	saveGen set true
	saveSim set true
	saveRec set true
	saveDet set true
	saveHit set true
	saveEmc set true
	resolution set 200
}
