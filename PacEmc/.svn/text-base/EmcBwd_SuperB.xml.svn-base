<?xml version="1.0" encoding="UTF-8" ?>
<edml>
    <included>
        <detector>
            <volume name="Calorimeter_Bwd">
                <include file="PacEmc/EmcDetBwd_SuperB.xml" />
            </volume>
        </detector>
        <!-- voxel parameter config section must have the same section name as the volume -->
        <config>
            <sect name="Calorimeter_Bwd">
                <param name="rbounds"    type="vector"        > 30.9 75.1 </param>
                <!-- no phi segmentation needed, as all elements are perfectly cylindrical -->
                <param name="nphivoxels"    type="vector"         > 1 </param>
                <param name="zbounds"   type="vector"         > -145 -141 -138 -134 -133   </param>
            </sect>
        </config>
    </included>
</edml>
