*
* Table that maps GEANT particle IDs to STDHEP IDs
*
* used by hep2geant.F
*
* Doug Wright, Nov 1995
*
      integer    STD_IDMAX
      parameter( STD_IDMAX = 49 )
      integer IDG2H(STD_IDMAX) / 22, -11, 11, 12, -13, 13, 111, 211
     +     , -211
     +     , 130, 321, -321, 2112, 2212, -2212, 310, 221, 3122, 3222
     +     , 3212, 3112, 3322, 3312, 3334, -2112, -3122, -3112, -3212
     +     , -3222, -3322, -3312, -3334, -15, 15, 411, -411, 421, -421
     +     , 431, -431, 4122, 24, -24, 23, 9991, 9992, 9993, 0, 9994
     +     /
      save IDG2H
*
*..Sun Solaris can only deal with 19 continuation characters per line :-(
*
*  1  +      22,  ! gamma
*  2  +     -11,  ! e+
*  3  +      11,  ! e-
*  4  +      12,  ! nu (nu_e)
*  5  +     -13,  ! mu+
*  6  +      13,  ! mu-
*  7  +     111,  ! pi0 
*  8  +     211,  ! pi+
*  9  +    -211,  ! pi-
* 10  +     130,  ! K0L
* 11  +     321,  ! K+
* 12  +    -321,  ! K-
* 13  +    2112,  ! n
* 14  +    2212,  ! p
* 15  +   -2212,  ! p~
* 16  +     310,  ! K0s
* 17  +     221,  ! eta
* 18  +    3122,  ! Lambda
* 19  +    3222,  ! Sigma+
* 20  +    3212,  ! Sigma0
* 21  +    3112,  ! Sigma-
* 22  +    3322,  ! Xi0
* 23  +    3312,  ! Xi-
* 24  +    3334,  ! Omega-
* 25  +   -2112,  ! n~
* 26  +   -3122,  ! Lambda~
* 27  +   -3112,  ! Sigma+~
* 28  +   -3212,  ! Sigma0~
* 29  +   -3222,  ! sigma-~
* 30  +   -3322,  ! Xi0~
* 31  +   -3312,  ! Xi-~
* 32  +   -3334,  ! Omega-~
* 33  +     -15,  ! tau+
* 34  +      15,  ! tau-
* 35  +     411,  ! D+
* 36  +    -411,  ! D-
* 37  +     421,  ! D0
* 38  +    -421,  ! D0~
* 39  +     431,  ! Ds+
* 40  +    -431,  ! Ds-
* 41  +    4122,  ! Lambda_c+
* 42  +      24,  ! W+
* 43  +     -24,  ! W-
* 44  +      23,  ! Z
* 45       9991,  ! deuteron    
* 46       9992,  ! tritium     
* 47       9993,  ! alpha       
* 48          0,  ! Geantino
* 49       9994,  ! He3         
