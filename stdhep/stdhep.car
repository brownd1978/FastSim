+DECK,TRLIST
      SUBROUTINE TRLIST(LUN)
C
C    list particle ID for stdhep 
C
      INTEGER ID1,ID2,ITMP1,ITMP2
      INTEGER LUN
      CHARACTER*20 HNAM1,HNAM2

C...special cases
      WRITE(LUN,1001)
      DO 100 I=1,100
        ITMP1=HEPCMP(I)
        IF(ITMP1.GT.0)THEN
          ID1=I
          CALL HEPNAM(ID1,HNAM1)
          ID2=-I
          ITMP2=HEPCMP(ID2)
          IF(ITMP2.EQ.0)THEN
            WRITE(LUN,1101) ID1,HNAM1,ITMP1
          ELSE
            CALL HEPNAM(ID2,HNAM2)
            WRITE(LUN,1102) ID1,HNAM1,ITMP1,ID2,HNAM2,ITMP2
          ENDIF
        ENDIF
 100  CONTINUE
      DO 150 J=11,99
        I=J*10
        ITMP1=HEPCMP(I)
        IF(ITMP1.GT.0)THEN
          ID1=I
          CALL HEPNAM(ID1,HNAM1)
          ID2=-I
          ITMP2=HEPCMP(ID2)
          IF(ITMP2.EQ.0)THEN
            WRITE(LUN,1101) ID1,HNAM1,ITMP1
          ELSE
            CALL HEPNAM(ID2,HNAM2)
            WRITE(LUN,1102) ID1,HNAM1,ITMP1,ID2,HNAM2,ITMP2
          ENDIF
        ENDIF
 150  CONTINUE
C...diquarks
      WRITE(LUN,1002)
      DO 200 I=11,99
        DO 190 J=1,10
          ID1=100*I+J-1
          ITMP1=HEPCMP(ID1)
          IF(ITMP1.GT.0)THEN
            ID2=-ID1
            CALL HEPNAM(ID1,HNAM1)
            ITMP2=HEPCMP(ID2)
            IF(ITMP2.EQ.0)THEN
              WRITE(LUN,1101) ID1,HNAM1,ITMP1
            ELSE
              CALL HEPNAM(ID2,HNAM2)
              WRITE(LUN,1102) ID1,HNAM1,ITMP1,ID2,HNAM2,ITMP2
            ENDIF
          ENDIF
 190    CONTINUE
 200  CONTINUE
C...mesons
      WRITE(LUN,1003)
      DO 300 I=1,9
        DO 290 J=1,9
          DO 280 K=1,10
            DO 270 L=1,10
              ID1=10000*(L-1)+100*I+10*J+K-1
              ITMP1=HEPCMP(ID1)
              IF(ITMP1.GT.0)THEN
                ID2=-ID1
                CALL HEPNAM(ID1,HNAM1)
                ITMP2=HEPCMP(ID2)
                IF(ITMP2.EQ.0)THEN
                  WRITE(LUN,1101) ID1,HNAM1,ITMP1
                ELSE
                  CALL HEPNAM(ID2,HNAM2)
                  WRITE(LUN,1102) ID1,HNAM1,ITMP1,ID2,HNAM2,ITMP2
                ENDIF
              ENDIF
 270        CONTINUE
 280      CONTINUE
 290    CONTINUE
 300  CONTINUE
C...baryons
      WRITE(LUN,1004)
      DO 400 I=1,9
        DO 390 J=1,9
          DO 380 K=1,9
            DO 370 L=1,10
              ID1=1000*I+100*J+10*K+L-1
              ITMP1=HEPCMP(ID1)
              IF(ITMP1.GT.0)THEN
                ID2=-ID1
                CALL HEPNAM(ID1,HNAM1)
                ITMP2=HEPCMP(ID2)
                IF(ITMP2.EQ.0)THEN
                  WRITE(LUN,1101) ID1,HNAM1,ITMP1
                ELSE
                  CALL HEPNAM(ID2,HNAM2)
                  WRITE(LUN,1102) ID1,HNAM1,ITMP1,ID2,HNAM2,ITMP2
                ENDIF
              ENDIF
 370        CONTINUE
 380      CONTINUE
 390    CONTINUE
 400  CONTINUE
      RETURN
 1001 FORMAT(//5X,'Special Cases'
     1     /4X,'ISTD  HNAM',14X,'ICOMP',8X,'ISTD  HNAM',14X,'ICOMP')
 1002 FORMAT(//5X,'Diquarks'
     1     /4X,'ISTD  HNAM',14X,'ICOMP',8X,'ISTD  HNAM',14X,'ICOMP')
 1003 FORMAT(//5X,'Mesons'
     1     /4X,'ISTD  HNAM',14X,'ICOMP',8X,'ISTD  HNAM',14X,'ICOMP')
 1004 FORMAT(//5X,'Baryons'
     1     /4X,'ISTD  HNAM',14X,'ICOMP',8X,'ISTD  HNAM',14X,'ICOMP')
 1101 FORMAT(1X,I7,2X,A15,1X,I7)
 1102 FORMAT(1X,I7,2X,A15,1X,I7,5X,I7,2X,A15,1X,I7)
      END

