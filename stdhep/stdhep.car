      PROGRAM TSTISA
C   
C     JTDKY = +/- unit number for decay table file.    
C                 If it is negative, decay table is not printed.   
C     JTEVT = +/- unit number for output event file.   
C                 If it is negative, only stable particles are written on it. 
C     JTCOM =     unit number for command file.    
C     JTLIS =     unit number for listing. 
C   
      IMPLICIT NONE 
+SEQ,ITAPES
+SEQ,IDRUN
+SEQ,PRIMAR
+SEQ,ISLOOP
C
+SEQ,HEPEVTC
+SEQ,HEPLUN
+SEQ,QUEST,IF=HPZTST,IF=UNIX.
      INTEGER IQUEST,IERR
C   
      INTEGER JTDKY,JTEVT,JTCOM,JTLIS,IFL,ILOOP,IPRT,INDEC
      CHARACTER*132 ISADEC
      LOGICAL OK,DONE,LOK
      SAVE /ITAPES/,/IDRUN/,/PRIMAR/,/ISLOOP/
      SAVE ILOOP,JTDKY,JTEVT,JTCOM,JTLIS

      EXTERNAL ALDATA
C   
C          Initialize ISAJET    
C   
      JTDKY=-1
      JTEVT=-2
      JTCOM=21
      JTLIS=22
+SELF,IF=UNIX
C  point to standard decay tables
      CALL GETENV( 'ISAJET_DIR', ISADEC )
      IF ( ISADEC .EQ. ' ' ) THEN
         ISADEC = 'isadecay.dat'
      ELSE
         INDEC   = INDEX ( ISADEC , ' ' ) - 1
         ISADEC  = ISADEC(:INDEC) // '/isadecay.dat'
      ENDIF
      OPEN(UNIT=1,FILE=ISADEC,STATUS='OLD')
+SELF,IF=VAX
      OPEN(UNIT=1,FILE='ISAJET_DIR:ISADECAY.DAT',STATUS='OLD',READONLY)
+SELF
      OPEN(UNIT=2,FILE='ISAOUT.DAT',STATUS='NEW',FORM='UNFORMATTED')
      OPEN(UNIT=JTCOM,FILE='STDTEST.CMD',STATUS='OLD')
+SELF,IF=-HPZTST.
      OPEN(UNIT=JTLIS,FILE='ISAJET.DAT',STATUS='NEW')
+SELF,IF=HPZTST.
      OPEN(UNIT=JTLIS,FILE='ISAJETZ.DAT',STATUS='NEW')
+SELF
      CALL ISAINI(JTDKY,JTEVT,JTCOM,JTLIS)  
C initialize HEP logical units
      LNHWRT=23
      LNHRD=0
      LNHDCY=0
      LNHOUT=JTLIS
+SELF,IF=-HPZTST.
      OPEN(UNIT=LNHWRT,FILE='ISAJET.IO',STATUS='NEW',FORM='UNFORMATTED')
+SELF,IF=HPZTST.
      CALL HEPZINIT
+SELF.
+SELF,IF=HPZTST,IF=-UNIX.
      OPEN(UNIT=LNHWRT,FILE='ISAJET.FZ',STATUS='NEW',RECORDTYPE='FIXED',
     1       FORM='UNFORMATTED',RECL=900,BLOCKSIZE=3600)
C...setup output file
      CALL FZFILE(LNHWRT,0,'XO')
+SELF.
+SELF,IF=HPZTST,IF=UNIX.
      CALL CFOPEN(IQUEST(1),0,900,'w',2,'ISAJET.FZ',IERR)
C...setup output file
      CALL FZFILE(LNHWRT,0,'XLO')
+SELF.
+SELF,IF=HEPDBG,IF=HPZTST.
      CALL FZLOGL(LNHWRT,3)
+SELF.
      CALL TSTBKEM(40)
      IPRT=1
C   
C print out list of defined particles and their translations
C    (you have to call ISAINI first)
C
      CALL ISAPRTLST(JTLIS)
C
C    compare particle ID for isajet and standard
C
      CALL HPTRLSTI
C   
C          Read instructions; terminate for STOP command or error.  
C   
 201  IFL=0 
      CALL ISABEG(IFL)  
      IF(IFL.NE.0)THEN
+SELF,IF=HPZTST.
C... close zebra file
        CALL FZENDO(LNHWRT,'ET')
+SELF.
C...print histograms
        CALL HOUTPU(LNHOUT)
        CALL HISTDO
      ENDIF
      IF(IFL.NE.0) STOP
C          Write begin-run record   
      CALL ISAWBG   
C   
C          Event loop   
C   
      ILOOP=0   
 301  CONTINUE  
      ILOOP=ILOOP+1   
C        Generate one event - discard if .NOT.OK  
      CALL ISAEVT(ILOOP,OK,DONE)  
C        Write event record   
      IF(OK)THEN
        CALL ISAHEP(1)
        IF(NEVHEP.LE.IPRT)THEN
           CALL PRTEVT(0)
           CALL HEPLST(1)
        ENDIF
        CALL TSTFLEM(40)
        CALL ISAWEV
+SELF,IF=-HPZTST.
        CALL HEPWRT(LOK)
+SELF,IF=HPZTST.
        CALL HEPZWRT(LOK)
+SELF.
      ENDIF
      IF(.NOT.DONE) GO TO 301   
C   
C          Calculate cross section and luminosity   
C   
      CALL ISAEND   
C          Write end-of-run record  
      CALL ISAWND   
      GO TO 201   
      END 

