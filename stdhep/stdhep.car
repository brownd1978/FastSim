+PATCH,TESTHST
+DECK,TSTFLEM
      SUBROUTINE TSTFLEM(IOFF)
C... fill histograms
+SEQ,HEPEVTC

      INTEGER IOFF,I

      CALL HFILL(IOFF+11,FLOAT(NEVHEP),0.,1.)
      CALL HFILL(IOFF+12,FLOAT(NHEP),0.,1.)
      DO 100 I=1,NHEP
        CALL HFILL(IOFF+13,FLOAT(IDHEP(I)),0.,1.)
        CALL HFILL(IOFF+14,FLOAT(ISTHEP(I)),0.,1.)
        CALL HFILL(IOFF+15,FLOAT(JMOHEP(1,I)),0.,1.)
        CALL HFILL(IOFF+16,FLOAT(JMOHEP(2,I)),0.,1.)
        CALL HFILL(IOFF+17,FLOAT(JDAHEP(1,I)),0.,1.)
        CALL HFILL(IOFF+18,FLOAT(JDAHEP(2,I)),0.,1.)
        CALL HFILL(IOFF+21,PHEP(1,I),0.,1.)
        CALL HFILL(IOFF+22,PHEP(2,I),0.,1.)
        CALL HFILL(IOFF+23,PHEP(3,I),0.,1.)
        CALL HFILL(IOFF+24,PHEP(4,I),0.,1.)
        CALL HFILL(IOFF+25,PHEP(5,I),0.,1.)
        CALL HFILL(IOFF+26,VHEP(1,I),0.,1.)
        CALL HFILL(IOFF+27,VHEP(2,I),0.,1.)
        CALL HFILL(IOFF+28,VHEP(3,I),0.,1.)
        CALL HFILL(IOFF+29,VHEP(4,I),0.,1.)
 100  CONTINUE
      RETURN
      END
+DECK,TSTBKEM
